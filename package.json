{
  "name": "monitoring-mtl-frontend",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "setupCoverage": "npm install --save-dev @istanbuljs/nyc-config-typescript",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run build && concurrently  \"npm run start\" \"npm run cypress:test\" --kill-others",
    "ci:test": " bash scripts/ci_tests.sh",
    "cypress:e2e": "nyc --require istanbul-lib-instrument --reporter=lcov --reporter=text-summary cypress run",
    "cypress:component": "cypress run --component",
    "cypress:open": "cypress open",
    "cypress:test": "npm run cypress:component && wait-port localhost:3000 && npm run cypress:e2e"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.16",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "formik": "^2.4.5",
    "next": "13.5.4",
    "ol": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "wait-port": "^1.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/d3": "^7.4.2",
    "@types/node": "^20",
    "@types/openlayers": "^4.6.20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "istanbul-lib-instrument": "^6.0.1",
    "nyc": "^15.1.0",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
